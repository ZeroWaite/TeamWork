<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="all">
        <item name="android:width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:textColor">@color/color_selector</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="fg">
        <item name="android:checked">true</item>
        <item name="android:textColor">@color/minechild_color_selector</item>

    </style>

<style name="Popwindow" parent="Animation.AppCompat.Dialog">
       <item name="android:windowEnterAnimation">@anim/show_animotion</item>
    <item name="android:windowExitAnimation">@anim/add_animotion</item>
</style>

<style name="Popwindow2" parent="Animation.AppCompat.Dialog">
    <item name="android:windowEnterAnimation">@anim/setuo_show_animotion</item>
    <item name="android:windowExitAnimation">@anim/setup_exit_animotion</item>
</style>

</resources>
